<Page x:Class="The_Versus.inGame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:The_Versus"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="inGame">

    <Grid>
        <Image Source="/img/forest.jpg" Stretch="Fill"/>
        <Image Margin="0,287,512,0" Source="/img/wizard.png" Stretch="Fill" Height="163" VerticalAlignment="Top" HorizontalAlignment="Right" Width="143"/>
        <Image Margin="507,245,0,0" Source="/img/orc.png" Stretch="Fill" RenderTransformOrigin="0.5,0.5" Height="205" VerticalAlignment="Top" HorizontalAlignment="Left" Width="221">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</Page>
